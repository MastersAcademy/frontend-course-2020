{
  "name": "webpack-config-N_Yarovenko",
  "version": "1.0.0",
  "description": "homeworks",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json",
    "test": "lint-staged",
    "test-all": "npm run test-js && npm run test-css",
    "test-js": "eslint --config .eslintrc.js ./src/*.js",
    "test-css": "stylelint --config .stylelintrc ./src/styles/*.{css,scss}"
  },
  "browserslist": "> 0.25%, not dead",
  "keywords": [
    "js",
    "javascript",
    "webpack"
  ],
  "author": "Nikolay Yarovenko",
  "license": "ISC",
  "eslintIgnore": [
    "*dist",
    "*node_modules",
    "*webpack.config.js",
    "*webpack.config.dev.js"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --config .eslintrc.js"
    ],
    "*.{css,scss}": [
      "stylelint --config .stylelintrc"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.6.0",
    "csv-loader": "^3.0.3",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-no-jquery": "^2.5.0",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.12.2",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "papaparse": "^5.3.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.3.0",
    "stylelint": "^13.7.1",
    "terser-webpack-plugin": "^2.3.8",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rxjs": "^6.6.3"
  }
}
